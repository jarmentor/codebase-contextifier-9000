{
  "python": {
    "extensions": [".py", ".pyw"],
    "tree_sitter_language": "python",
    "chunk_types": {
      "function_definition": {
        "name_field": "name",
        "docstring_field": "body.expression_statement.string"
      },
      "class_definition": {
        "name_field": "name",
        "docstring_field": "body.expression_statement.string"
      },
      "decorated_definition": {
        "name_field": "definition.name"
      }
    },
    "comment_patterns": ["#"]
  },
  "javascript": {
    "extensions": [".js", ".jsx", ".mjs", ".cjs"],
    "tree_sitter_language": "javascript",
    "chunk_types": {
      "function_declaration": {
        "name_field": "name"
      },
      "function": {
        "name_field": "name"
      },
      "arrow_function": {
        "name_field": null
      },
      "class_declaration": {
        "name_field": "name"
      },
      "method_definition": {
        "name_field": "name"
      }
    },
    "comment_patterns": ["//", "/*"]
  },
  "typescript": {
    "extensions": [".ts", ".tsx"],
